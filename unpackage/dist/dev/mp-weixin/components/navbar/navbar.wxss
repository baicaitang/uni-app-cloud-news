.navbar .navbar-fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 99;
  width: 100%;
  background-color: #f07373;
}
.navbar .navbar-fixed .navbar-content {
  padding: 0 15px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  box-sizing: border-box;
}
.navbar .navbar-fixed .navbar-content .navbar-search {
  padding: 0 10px;
  height: 30px;
  width: 100%;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-radius: 30px;
  box-sizing: border-box;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search-icon {
  margin-right: 10px;
}
.navbar .navbar-fixed .navbar-content .navbar-search .navbar-search-text {
  width: 100%;
  font-size: 14px;
  color: #999;
}
.navbar .navbar-fixed .navbar-content.issearch {
  padding-left: 0;
}
.navbar .navbar-fixed .navbar-content.issearch .navbar-content-search-icons {
  margin: 0 10px;
}
.navbar .navbar-fixed .navbar-content.issearch .navbar-search {
  border-radius: 5px;
}

